{
    "name": "jakubonderka/php-var-dump-check",
    "license": "BSD-2-Clause",
    "description": "Find forgotten variables dump in PHP source code.",
    "authors": [
        {
            "name": "Jakub Onderka",
            "email": "jakub.onderka@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {"JakubOnderka\\PhpVarDumpCheck": "src/"}
    },
    "require": {
        "php": ">=5.3.2"
    },
    "suggest": {
        "jakub-onderka/php-console-highlighter": "For colored console output"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "bin": ["var-dump-check"]
}
